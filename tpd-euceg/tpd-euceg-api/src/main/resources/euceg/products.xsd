<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" elementFormDefault="qualified"
           jaxb:version="2.1">

    <xs:include schemaLocation="attachments.xsd"/>
    <xs:include schemaLocation="common.xsd"/>

    <xs:complexType name="Product" abstract="true">
        <xs:annotation>
            <xs:documentation>Defines an abstract product.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ProductID" type="ProductNumber">
                <xs:annotation>
                    <xs:documentation>Product ID is the identification number of the product used in the system in the format "Submitter
                        ID-Year-Product number" (NNNNN-NN-NNNNN), where: - "Submitter ID" is the ID number of the submitter. - "Year" is the
                        year within which data on the product were submitted for the first time (2 digits). - "Product number" is the number
                        attributed by the submitter to the product when submitting data for the first time.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PreviousProductID" type="ProductNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>In the case of substantial modification to an already notified product (entailing a new Product ID),
                        indication of the previous Product ID of the product.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OtherProductsExist" type="BooleanNullable">
                <xs:annotation>
                    <xs:documentation>Indication whether the submitter is aware of other product(s) with the same design and composition
                        that is marketed in the EU using a different Product ID.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OtherProducts" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>List Product ID of the product(s) with same design and composition. If Product ID of the product(s) is
                        not known to the submitter, full brand and subtype name(s) as well as Member State(s) where product(s) is placed on
                        the market shall at least be provided.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ProductIdentification" type="ProductIdentification" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>The other product identification.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="SameCompositionProductsExist" type="BooleanNullable">
                <xs:annotation>
                    <xs:documentation>Indication whether the submitter is aware of other product(s) with the same composition of e-liquid,
                        but different design.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SameCompositionProducts" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The Product ID of the other product(s) with the same proportion of ingredients in the tobacco blends
                        composition known to the submitter.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="ProductIdentification" type="ProductIdentification" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>The same composition product identification.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Manufacturers" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>If the Submitter is not the manufacturer, the official company name of the manufacturer of the product
                        including its company details (multiple entries possible if the product is manufactured by multiple companies).
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Manufacturer" type="Manufacturer" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="MarketResearchFiles" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The internal and external studies on market research and preferences of various consumer groups,
                        including young people and current smokers, relating to ingredients and emissions, available to submitter as well as
                        executive summaries of any market surveys carried out when launching new products. To be updated in case new data
                        become available.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>                        
                        <xs:element name="Attachment" type="AttachmentRef" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ProductNumber">
        <xs:annotation>
            <xs:documentation>Defines the product ID (TP-ID or EC-ID).</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="ProductNumberValue">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the product ID is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="ProductNumberValue">
        <xs:annotation>
            <xs:documentation>Defines the product ID (TP-ID or EC-ID).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{5}-\d{2}-\d{5}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="ProductIdentification">
        <xs:annotation>
            <xs:documentation>Defines a product identification.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="String1000">
                <xs:attribute name="type" type="ProductIdentificationType" use="required">
                    <xs:annotation>
                        <xs:documentation>The type of the product identification.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="ProductIdentificationType">
        <xs:annotation>
            <xs:documentation>The list of product identifications.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="PRODUCT_ID">
                <xs:annotation>
                    <xs:documentation>The product identification value is a product ID.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="TEXT">
                <xs:annotation>
                    <xs:documentation>The product identification value is a free text.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="Manufacturer">
        <xs:annotation>
            <xs:documentation>Defines a manufacturer of a product.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="Company">
                <xs:sequence>
                    <xs:element name="ProductionSiteAddresses">
                        <xs:annotation>
                            <xs:documentation>For the manufacturer, address(es) of the site(s) where production is completed.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="ProductionSiteAddress" type="ProductionSiteAddress" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ProductionSiteAddress">
        <xs:annotation>
            <xs:documentation>Define a production site address.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Address" type="String300Value">
                <xs:annotation>
                    <xs:documentation>The address of the production site.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Country" type="CountryValue">
                <xs:annotation>
                    <xs:documentation>The country of the production site.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="PhoneNumber" type="PhoneNumberValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The business phone of the production site.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Email" type="EmailAddressValue" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The email address of the production site.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="confidential" type="xs:boolean" use="required">
            <xs:annotation>
                <xs:documentation>Indicates whether or not the production site address is confidential.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="Presentation" abstract="true">
        <xs:annotation>
            <xs:documentation>Defines an abstract product presentation details.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="NationalMarket" type="NationalMarket">
                <xs:annotation>
                    <xs:documentation>The Member State to which the product information below is being provided.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="NationalComment" type="String1000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The national comment.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BrandName" type="String100">
                <xs:annotation>
                    <xs:documentation>The brand name under which the product is marketed in the Member State(s) to which information is
                        being submitted.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BrandSubtypeNameExists" type="Boolean">
                <xs:annotation>
                    <xs:documentation>Indicates if the product "subtype name" exists.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="BrandSubtypeName" type="String100" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The product "subtype name" (if any) as marketed in the Member State to which the product information
                        is being submitted.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="LaunchDate" type="Date">
                <xs:annotation>
                    <xs:documentation>The date on which the Submitter plans to launch/launched the product on the market.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="WithdrawalIndication" type="Boolean">
                <xs:annotation>
                    <xs:documentation>Indicates if the Submitter plans to withdraw/withdrew the product from the market.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="WithdrawalDate" type="Date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The date on which the Submitter plans to withdraw/withdrew the product from the market.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProductNumberType" type="PresentationNumberType">
                <xs:annotation>
                    <xs:documentation>The identifier number of the product presentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProductNumber" type="String40">
                <xs:annotation>
                    <xs:documentation>The identifier number of the product presentation.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PresentationNumberType">
        <xs:annotation>
            <xs:documentation>Defines a presentation number type.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="PresentationNumberTypeEnum">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the presentation number type is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="PresentationNumberTypeEnum">
        <xs:annotation>
            <xs:documentation>The list of presentation number types.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="SUBMITTER">
                <xs:annotation>
                    <xs:documentation>The identifier number used internally by the submitter.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="UPC">
                <xs:annotation>
                    <xs:documentation>The UPC-12 (Universal Product Code) of the product.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="EAN">
                <xs:annotation>
                    <xs:documentation>The EAN-13 or EAN-8, (European Article Number) of the product.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="GTIN">
                <xs:annotation>
                    <xs:documentation>GTIN (Global Trade Identification Number) of the product.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="SKU">
                <xs:annotation>
                    <xs:documentation>SKU (Stock Keeping Unit) number(s) of the product.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="AnnualSalesData" abstract="true">
        <xs:annotation>
            <xs:documentation>Defines an abstract annual sales data.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Year" type="Year">
                <xs:annotation>
                    <xs:documentation>The year of the annual sales volume.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SalesVolume" type="Double">
                <xs:annotation>
                    <xs:documentation>The information on annual sales volume of the product per Member State to be reported annually.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Ingredient" abstract="true">
        <xs:annotation>
            <xs:documentation>Defines an abstract ingredient.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TemplateID" type="UUID" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient template if used.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TemplateIDUpdate" type="Date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Last refresh from template repository</xs:documentation>
                </xs:annotation>
            </xs:element>            
            <xs:element name="Name" type="String300">
                <xs:annotation>
                    <xs:documentation>The chemical name of the ingredient.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CasNumberExists" type="BooleanNullable" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Indicates if the CAS (Chemical Abstracts Service) number exists.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CasNumber" type="CasNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The CAS number.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AdditionalCasNumbers" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The additional CAS numbers if applicable and cannot be completed if the CAS number is empty.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CasNumber" type="CasNumber" maxOccurs="10"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="FemaNumber" type="FemaNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The FEMA (Flavour and Extract Manufacturers Association) number if any.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AdditiveNumber" type="AdditiveNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>If the ingredient is a food additive, its food additive “E number” set out in Annexes II and III to
                        Regulation (EC) No 1333/2008 of the European Parliament and of the Council.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="FlNumber" type="FlNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The FL number, if any (European Flavouring number as set out in Annex I to Regulation (EC) No
                        1334/2008 of the European Parliament and of the Council).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EcNumber" type="EcNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The European Community (EC) number, if any.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OtherNumber" type="String40" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>ID number used internally by the submitter.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RecipeQuantity" type="Double">
                <xs:annotation>
                    <xs:documentation>The standard weight of the ingredient included in one product unit in mg according to recipe.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Functions" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The functions of the ingredient.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Function" type="IngredientFunction" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="FunctionOther" type="String100" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The function of the ingredient if "Other".</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ToxicityStatus" type="ToxicityStatus">
                <xs:annotation>
                    <xs:documentation>Indication whether the ingredient (in unburnt/non-vaporised form) status is characterised by any known
                        type of toxicity or has carcinogenic, mutagenic or toxic for reproduction properties..
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="ReachRegistration" type="ReachRegistration" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Indication whether the REACH registration number might be not available for substances.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="ReachRegistrationNumber" type="String40" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>The registration number pursuant to Regulation (EC) No 1907/2006 of the European Parliament and of
                            the Council, if any.
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element name="ClpWhetherClassification" type="BooleanNullable" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Indicates if the ingredient has been classified under Regulation (EC) No 1272/2008 of the European
                        Parliament and of the Council and is in the classification and minOccurs="0" labelling inventory.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpAcuteToxOral" type="AcuteToxOralCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to acute oral toxicity based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpAcuteToxDermal" type="AcuteToxDermalCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to acute dermal toxicity based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpAcuteToxInhalation" type="AcuteToxInhalationCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to acute inhalation toxicity based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpSkinCorrosiveIrritant" type="SkinCorrosiveIrritantCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification as a skin corrosive/irritant based on Regulation (EC) No 1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpEyeDamageIrritation" type="EyeDamageIrritationCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification as responsible for eye damage/irritation based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpRespiratorySensitisation" type="RespiratorySensitisationCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to respiratory sensitisation based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpSkinSensitisation" type="SkinSensitisationCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to skin sensitisation based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpMutagenGenotox" type="MutagenGenotoxCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards mutagenicity/genotoxicity based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpCarcinogenicity" type="CarcinogenicityCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to its carcinogenicity based onRegulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpReproductiveTox" type="ReproductiveToxCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to its reproductive toxicity based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpStot" type="StotCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient classification with regards to its specific target organ toxicity based on Regulation
                        (EC) No 1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpStotDescription" type="String500" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The reference to the specific organ(s) affected based on the above classification.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ClpAspirationTox" type="AspirationToxCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The ingredient Classification with regards to aspiration toxicity based on Regulation (EC) No
                        1272/2008.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ToxicologicalDetails" type="ToxicologicalDetails">
                <xs:annotation>
                    <xs:documentation>The toxicological details of the ingredient.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CasNumber">
        <xs:annotation>
            <xs:documentation>Defines a CAS number.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="CasNumberValue">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the CAS number is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="CasNumberValue">
        <xs:annotation>
            <xs:documentation>Defines a CAS number.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{2,7}-\d{2}-\d"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="FemaNumber">
        <xs:annotation>
            <xs:documentation>Defines a FEMA number.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="FemaNumberValue">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the FEMA number is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="FemaNumberValue">
        <xs:annotation>
            <xs:documentation>Defines a FEMA number.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{4}"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="AdditiveNumber">
        <xs:annotation>
            <xs:documentation>Defines a food additive number.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="AdditiveNumberValue">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the food additive number is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="AdditiveNumberValue">
        <xs:annotation>
            <xs:documentation>Defines a food additive number.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="5"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="FlNumber">
        <xs:annotation>
            <xs:documentation>Defines a FL number.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="FlNumberValue">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
                    <xs:annotation>
                        <xs:documentation>Indicates whether or not the FL number is confidential.</xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

