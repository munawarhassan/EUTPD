    <xs:complexType name="Emission" abstract="true">
        <xs:annotation>
            <xs:documentation>Defines an abstract product emission.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CasNumber" type="CasNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The CAS (Chemical Abstracts Service) number of the other emissions.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IupacName" type="String100" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The IUPAC (International Union of Pure and Applied Chemistry) name of the other emissions, should a
