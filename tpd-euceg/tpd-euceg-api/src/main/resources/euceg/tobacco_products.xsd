<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
	xmlns:annox="http://annox.dev.java.net" elementFormDefault="qualified"
	jaxb:version="2.1" jaxb:extensionBindingPrefixes="xjc annox">

	<xs:include schemaLocation="products.xsd" />

	<xs:complexType name="TobaccoProduct">
		<xs:annotation>
			<xs:documentation>Defines a tobacco product.</xs:documentation>
			<xs:appinfo>
				<jaxb:bindings
					node="//xs:element[@name='CigaretteSpecific']">
					<annox:annotate target="field">
						<annox:annotate
							annox:class="org.eclipse.persistence.oxm.annotations.XmlNullPolicy"
							emptyNodeRepresentsNull="true" />
					</annox:annotate>
				</jaxb:bindings>
				<jaxb:bindings
                    node="//xs:element[@name='SmokelessSpecific']">
                    <annox:annotate target="field">
                        <annox:annotate
                            annox:class="org.eclipse.persistence.oxm.annotations.XmlNullPolicy"
                            emptyNodeRepresentsNull="true" />
                    </annox:annotate>
                </jaxb:bindings>
                <jaxb:bindings
                    node="//xs:element[@name='RyoPipeSpecific']">
                    <annox:annotate target="field">
                        <annox:annotate
                            annox:class="org.eclipse.persistence.oxm.annotations.XmlNullPolicy"
                            emptyNodeRepresentsNull="true" />
                    </annox:annotate>
                </jaxb:bindings>
                <jaxb:bindings
                    node="//xs:element[@name='NovelSpecific']">
                    <annox:annotate target="field">
                        <annox:annotate
                            annox:class="org.eclipse.persistence.oxm.annotations.XmlNullPolicy"
                            emptyNodeRepresentsNull="true" />
                    </annox:annotate>
                </jaxb:bindings>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="Product">
				<xs:sequence>
					<xs:element name="ProductType" type="TobaccoProductType">
						<xs:annotation>
							<xs:documentation>The product type.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Length" type="Double" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The average length of the product unit in mm.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Diameter" type="Double" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The average diameter (measured at the point with maximal diameter) of the product unit in
								mm.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Weight" type="Double">
						<xs:annotation>
                            <xs:documentation>The weight of one product unit, including the specified moisture, in mg.</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element name="TobaccoWeight" type="Double" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The total weight of the tobacco in one product in mg. Remark: a unit of product is one cigar,
                                one cigarillo, one cigarette, one pouch/bit, etc. of SLT. One unit for all loose products is 1 g including
                                the moisture content and expressed in mg.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element name="Filter" type="BooleanNullable" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>Indicates the existence of a filter in the product.</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element name="FilterLength" type="Integer" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The length of the product filter in mm.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TechnicalFiles" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The technical document(s) setting out a general description of the additives used and their
								properties.
							</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
                                <xs:element name="Attachment" type="AttachmentRef" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Presentations">
						<xs:annotation>
                            <xs:documentation>Description of the format(s) and Member State(s) in which the product is presented for sale.
							</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
                                <xs:element name="Presentation" type="TobaccoPresentation" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="TobaccoIngredients" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The tobacco ingredients defined by leaf cure method, leaf type and part type used in the
								product.
							</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
                                <xs:element name="TobaccoIngredient" type="TobaccoIngredient" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="OtherIngredientsExist" type="Boolean">
						<xs:annotation>
                            <xs:documentation>Indication whether other ingredients exist.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OtherIngredients" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The other ingredient means an additive, as well as any substance or element present in a
                                finished tobacco product or related products, including paper, filter, ink, capsules and adhesives.
							</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
                                <xs:element name="Ingredient" type="TobaccoOtherIngredient" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
                    <xs:element name="TncoEmission" type="TncoEmission" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The TNCO emission information.</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element name="OtherEmissionsAvailable" type="Boolean">
						<xs:annotation>
                            <xs:documentation>The indication as to whether other emissions have been measured.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OtherEmissions" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The emissions of the product.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
                                <xs:element name="Emission" type="TobaccoEmission" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:choice minOccurs="0">
                        <xs:element name="CigaretteSpecific" type="CigaretteSpecific">
							<xs:annotation>
                                <xs:documentation>The information on the cigarette filter.</xs:documentation>
							</xs:annotation>
						</xs:element>
                        <xs:element name="SmokelessSpecific" type="SmokelessSpecific">
							<xs:annotation>
                                <xs:documentation>The information the smokeless product.</xs:documentation>
							</xs:annotation>
						</xs:element>
                        <xs:element name="RyoPipeSpecific" type="RyoPipeSpecific" minOccurs="0">
							<xs:annotation>
                                <xs:documentation>The information on the roll your own tobacco and pipe tobacco products.</xs:documentation>
							</xs:annotation>
						</xs:element>
                        <xs:element name="NovelSpecific" type="NovelSpecific" minOccurs="0">
							<xs:annotation>
                                <xs:documentation>The information on the product tobacco and pipe tobacco products.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TobaccoProductType">
		<xs:annotation>
            <xs:documentation>Defines the tobacco product type.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="TobaccoProductTypeEnum">
                <xs:attribute name="confidential" type="xs:boolean" use="required">
					<xs:annotation>
                        <xs:documentation>Indicates whether or not the product type is confidential.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

