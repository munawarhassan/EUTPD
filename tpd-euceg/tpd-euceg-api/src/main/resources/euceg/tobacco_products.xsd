				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="10">
				<xs:annotation>
					<xs:documentation>Herbal product for smoking</xs:documentation>
					<xs:appinfo>
                        <jaxb:typesafeEnumMember name="HERBAL_PRODUCT"/>
					</xs:appinfo>
				</xs:annotation>
