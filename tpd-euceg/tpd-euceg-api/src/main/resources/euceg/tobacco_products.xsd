		<xs:restriction base="xs:int">
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Cigarette</xs:documentation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="CIGARETTE" />
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Cigar</xs:documentation>
					<xs:appinfo>
						<jaxb:typesafeEnumMember name="CIGAR" />
