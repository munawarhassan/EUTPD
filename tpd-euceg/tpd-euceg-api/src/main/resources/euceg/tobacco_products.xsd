	<xs:complexType name="SmokelessSpecific">
		<xs:annotation>
            <xs:documentation>Defines the specific information for smokeless products.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="RyoPipeSpecific">
				<xs:sequence>
					<xs:element name="Ph" type="Double">
						<xs:annotation>
							<xs:documentation>The pH of the product.</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element name="TotalMoisture" type="Double" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The total moisture of the product per product unit.</xs:documentation>
						</xs:annotation>
					</xs:element>
                    <xs:element name="AnalysisMethods" type="String300" minOccurs="0">
						<xs:annotation>
                            <xs:documentation>The brief Text description of the methods used to measure the above variables including
                                references to more detailed information on the approach.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="RyoPipeSpecific">
		<xs:annotation>
            <xs:documentation>Defines specific information for Roll-your-own / Pipe products.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TotalNicotineContent" type="Double">
				<xs:annotation>
                    <xs:documentation>The total nicotine content of the product per product unit.</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="UnionisedNicotineContent" type="Double" minOccurs="0">
				<xs:annotation>
                    <xs:documentation>The unionised nicotine content of the product per product unit.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="NovelSpecific">
		<xs:annotation>
            <xs:documentation>Defines specific information for novel tobacco product.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
            <xs:element name="DetailsDescriptionFile" type="AttachmentRef" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>A PDF file describing the available scientific studies on toxicity, addictiveness and attractiveness
                        of the novel tobacco product, in particular as regards its ingredients and emissions.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="UseInstructionsFile" type="AttachmentRef" minOccurs="0">
				<xs:annotation>
                    <xs:documentation>A PDF file describing the available studies, executive summaries thereof and market research on the
                        preferences of various consumer groups, including young people and current smokers.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
            <xs:element name="RiskBenefitFile" type="AttachmentRef" minOccurs="0">
				<xs:annotation>
                    <xs:documentation>The pdf file describing the other available and relevant information, including a risk/benefit
                        analysis of the product, its expected effects on cessation of tobacco consumption, its expected effects on
                        initiation of tobacco consumption and predicted consumer perception.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StudyFiles" minOccurs="0">
				<xs:annotation>
                    <xs:documentation>Other available studies/information as provided for under Article 19 of the TPD and not submitted in
                        other sections.
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
                        <xs:element name="Attachment" type="AttachmentRef" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>