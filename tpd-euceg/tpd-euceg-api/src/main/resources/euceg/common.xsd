                    printMethod="com.pmi.tpd.euceg.api.Parsers.printDateTime" />
                <jaxb:javaType name="org.joda.time.LocalDate" xmlType="xs:date"
                    parseMethod="com.pmi.tpd.euceg.api.Parsers.parseLocalDate"
                    printMethod="com.pmi.tpd.euceg.api.Parsers.printLocalDate" />
